{
  "name": "operaton-cockpit-jupyterlite-packages",
  "private": true,
  "workspaces": [
    "packages/operaton-extension",
    "packages/jupyterlab-bpmn",
    "packages/jupyterlab-dmn"
  ],
  "scripts": {
    "build": "npm run build -w @operaton/operaton-extension && npm run build:lib -w jupyterlab-bpmn && npm run build:lib -w jupyterlab-dmn",
    "clean": "npm run clean --workspaces --if-present"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
