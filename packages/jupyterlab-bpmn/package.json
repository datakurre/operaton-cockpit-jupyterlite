{
  "name": "jupyterlab-bpmn",
  "version": "0.5.6",
  "description": "A JupyterLab extension for rendering BPMN files",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/datakurre/jupyterlab-bpmn",
  "bugs": {
    "url": "https://github.com/datakurre/jupyterlab-bpmn/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Asko Soukka",
    "email": "asko.soukka@iki.fi"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/datakurre/jupyterlab-bpmn.git"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:labextension:dev",
    "build:prod": "npm run clean && npm run build:lib && npm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyterlab_bpmn/labextension",
    "clean:all": "npm run clean && npm run clean:labextension"
  },
  "dependencies": {
    "@jupyterlab/rendermime-interfaces": "^3.11.0",
    "bpmn-js": "^18.10.1",
    "bpmn-moddle": "^10.0.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "diagram-js": "^15.6.0",
    "html-to-image": "^1.11.11",
    "svg-curves": "^1.0.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.3.0",
    "rimraf": "^6.0.0",
    "typescript": "~5.3.0"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "mimeExtension": true,
    "outputDir": "jupyterlab_bpmn/labextension"
  }
}
