{
  "name": "jupyterlab-dmn",
  "version": "0.2.0",
  "description": "A JupyterLab extension for rendering DMN files",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/datakurre/jupyterlab-dmn",
  "bugs": {
    "url": "https://github.com/datakurre/jupyterlab-dmn/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Asko Soukka",
    "email": "asko.soukka@iki.fi"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/datakurre/jupyterlab-dmn.git"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:labextension:dev",
    "build:prod": "npm run clean && npm run build:lib && npm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyterlab_dmn/labextension",
    "clean:all": "npm run clean && npm run clean:labextension"
  },
  "dependencies": {
    "@jupyterlab/rendermime-interfaces": "^3.11.0",
    "dmn-js": "^17.5.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.3.0",
    "rimraf": "^6.0.0",
    "typescript": "~5.3.0"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "mimeExtension": true,
    "outputDir": "jupyterlab_dmn/labextension"
  }
}
